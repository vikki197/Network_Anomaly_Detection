ost srv count of flags\n","df['srvserror'] = df['srvcount']*df['srvrerrorrate']\n","df['srvrerror'] = df['srvcount']*df['srvserrorrate']\n","\n","df['samesrv'] = df['count']*df['samesrvrate']\n","df['diffsrv'] = df['count']*df['diffsrvrate']"],"metadata":{"id":"XsjZXt_zPH7i","executionInfo":{"status":"ok","timestamp":1735924462646,"user_tz":-330,"elapsed":36,"user":{"displayName":"Vamsi Krishna","userId":"09981900051289340897"}}},"execution_count":27,"outputs":[]},{"cell_type":"code","source":["smean = df.groupby('service').agg(count_mean=('count','mean'),srvcount_mean=('srvcount','mean'),\n","                          serror_mean = ('serror','mean'), rerror_mean = ('rerror','mean'),\n","                          srvserror_mean = ('srvserror','mean'), srvrerror_mean = ('srvrerror','mean'),\n","                          samesrv_mean = ('samesrv','mean'), diffsrv_mean = ('diffsrv','mean')).reset_index()\n","smean"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":423},"id":"bl1Rla3_P6kd","executionInfo":{"status":"ok","timestamp":1735924462646,"user_tz":-330,"elapsed":36,"user":{"displayName":"Vamsi Krishna","userId":"09981900051289340897"}},"outputId":"976c0c17-781f-47ee-ce82-62603e916738"},"execution_count":28,"outputs":[{"output_type":"execute_result","data":{"text/plain":["      service  count_mean  srvcount_mean  serror_mean  rerror_mean  \\\n","0         IRC    4.181818       1.112299     0.330321     3.353262   \n","1         X11   15.726027       1.410959     4.661096     9.941507   \n","2      Z39_50  213.140371      10.882831   170.605557    42.522923   \n","3         aol  511.000000       1.000000    40.880000   470.120000   \n","4        auth  169.205236       7.731937   135.881372    32.952901   \n","..        ...         ...            ...          ...          ...   \n","65      urp_i   11.036545       2.827243     0.000000     0.000000   \n","66       uucp  118.708974      10.716667    93.616500    25.068423   \n","67  uucp_path  214.